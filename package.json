{
  "name": "wundergraph-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p dev wundergraph open",
    "wundergraph": "wunderctl up --logs",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "build:wundergraph": "wunderctl generate",
    "build:next": "next build",
    "build": "npm run build:wundergraph && npm run build:next",
    "dev": "next dev",
    "check": "tsc --noEmit",
    "test": "jest",
    "test:playwright": "npx -- playwright test"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.6",
    "@mui/x-date-pickers": "^6.9.0",
    "@wundergraph/nextjs": "^0.11.11",
    "@wundergraph/sdk": "^0.166.0",
    "axios": "^1.4.0",
    "busboy": "^1.6.0",
    "graphql": "^16.3.0",
    "next": "^13.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@playwright/test": "^1.28.1",
    "@types/busboy": "^1.5.0",
    "@types/node": "^17.0.15",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "prettier": "2.8.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1"
  }
}
